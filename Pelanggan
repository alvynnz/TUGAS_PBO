public class pelanggan{
    private int id_pelanggan;
    private varchar nama;
    private varchar alamat;
    private varchar no_telepon;

    public pelanggan (int id_pelanggan,varchar nama, varchar alamat, varchar no_telepon){
        this.id_pelanggan=id_pelanggan;
        this.nama=nama;
        this.alamat=alamat;
        this.no_telepon=no_telepon;
    
    }

    public int get id_pelanggan()(
        retrun id_pelanggan;
    )

    public void set id_pelanggan(int id_pelanggan){
        thus.id_pelanggan = id_pelanggan;

    }
    public varchar get nama(){
        retrun nama;

    }
    public void set nama(varchar nama){
        this.nama=nama;

    }
    public varchar get alamat(){
        retrun alamat;

    }
    public void set alamat(varchar alamat){
        thsi.alamat=alamat;

    }
    public varchar get no_telepon(){
        retrun no_telepon;

    }
    public void set no_telepon(){
        this.no_telepon=no_telepon;

    }

    public varchar getinfoPelanggan(){
        retrun "pelanggan [id="+id_pelanggan + "Nama=" + nama + "Alamat" +alamat + "No telepon" + no_telepon +"]"
    }
}